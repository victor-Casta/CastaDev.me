<style>
    @tailwind base;
    @tailwind components;
    @tailwind utilities;
    
    .Projects {
        @apply w-full h-full grid grid-cols-1 grid-rows-1 md:grid-cols-2;
        .github__Projects {
            @apply w-full md:w-[95%] h-[298px] md:h-full mb-4 md:mb-0 bg-green-400 rounded-xl flex justify-start px-4 py-4 bg-no-repeat bg-center ;
            background-image: url(../assets/img/gif_3.gif);
            h2 {
                @apply font-anton tracking-wide text-black font-bold text-2xl mr-3;
            }
            .github-icon {
                @apply w-8 h-8;
            }
        }
        .main__Projects {
            @apply w-[100%] h-full grid grid-cols-1 grid-rows-3 gap-y-3 rounded-xl;
            .project {
                @apply w-full h-full flex flex-col justify-center py-5 px-4 bg-primary rounded-xl hover:bg-purple-600 cursor-pointer;
                .tecnologies {
                    @apply flex relative px-3;
                    .item {
                        @apply w-10 h-10 ml-[-13px] flex justify-center items-center bg-white rounded-[50%] border-2 border-black;
                    }
                }
                h2 {
                    @apply text-green_soft font-bold text-lg pb-1;
                }
                p {
                    @apply text-base text-slate-300 pb-2 leading-tight;
                }
            }
        }
    }
</style>